<% include headerfooter/header.ejs %>
<link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
<div class="container">
	<div class="row">
		<h1 style="text-align:center; margin-top: 100px">Login Here!</h1>
		<div style="width:30%; margin:25px auto;">
			<form action="#" method="POST">
				<div class="form-group">
					<input class="form-control" type="text" name="username" placeholder="username" required>
				</div>
				<div class="form-group">
					<input class="form-control" type="password" name="password" placeholder="password" required>
				</div>
				<div class="form-group" >
					<button class="btn btn-lg btn-default btn-block btn-primary" style="margin-top: 0; border:0;"> Login!!</button>
				</div>
			</form>
			<% var str  %>

			<p><%= str %></p>
			<a href ="/homepage">go back</a>
			<a href ="/signup" style="margin-left: 10%">create new account</a>
		</div>
	</div>
</div>
